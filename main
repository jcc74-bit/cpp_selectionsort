#include <iostream>
#include <cstdlib>
#include <ctime>
using namespace std;
int main (){
    srand (time(0));
    int array[100];
    for (int i=0; i<100; i++){
        array[i]= i+1;
    }
    cout << "\n\n";
    for (int i=0; i<100; i++){
    int randIndex = rand() % 100;
    int temp = array[i];
    array[i]= array[randIndex];
    array[randIndex]= temp;

}
    for(int i=0; i<100; i++){
        cout << array[i]<< " ";
    }
    cout << "\n\n";
    
    for (int j=0; j<100-1; j++){
        int iMin = j;
        for(int i=j+1; i<100; i++){
            if(array[i]<array[iMin]){
                iMin=i;
            }
        }
    if (iMin != j){
        int temp = array[j];
        array[j] = array[iMin];
        array[iMin] = temp;
        }
    } 
     cout << "selection sort: ";
    for (int i = 0; i < 100; i++) {
        cout << array[i] << " ";
    }
    cout << "\n";
}


